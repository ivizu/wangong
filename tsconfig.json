{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "outDir": "build",
    "rootDir": "src",
    "moduleResolution": "node",
    "module": "CommonJS",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
